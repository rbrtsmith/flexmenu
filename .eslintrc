{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "rules": {
    "react/jsx-quotes": 0,
    "jsx-quotes": [2, "prefer-double"],
    "comma-dangle": 0,
    "max-len": 0,
    'import/no-extraneous-dependencies': [2, {
      devDependencies: true
    }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}